<!DOCTYPE html>
<html lang="pt-br">

<%- include('../partials/head'); %>

    <body class="container">
        <%- include('../partials/header'); %>
            <main>
                <div class="jumbotron">
                    <h1>Node com EJS</h1>
                    <p>Trabalhando com arquivos EJS</p>
                </div>
                <ul>
                    <% technologies.forEach(function(technologie) { %>
                        <li>
                            <strong>
                                <%= technologie.name %>
                            </strong>
                            representing <%= technologie.organization %>, born <%= technologie.birth_year %>
                        </li>
                        <% }); %>
                </ul>

            </main>

            <%- include('../partials/footer'); %>

    </body>

</html>